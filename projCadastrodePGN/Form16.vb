    Private Sub Button12_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button12.Click
        frmPRETASCORTADO.Show()

        Dim nomedomestre As String = "STOCKFISH"

        Select Case tipodapartida

            Case "PESXPC"

                nomebrancas = "PES/" & nomedomestre
                mestrebrancas = "ac"

            Case "PCXPC"
                nomebrancas = nomedomestre
                mestrebrancas = nomedomestre.Substring(0, 3).ToLower

        End Select

        Me.Close()
    End Sub

